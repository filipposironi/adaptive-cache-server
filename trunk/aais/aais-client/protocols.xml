<?xml version="1.0" encoding="utf-8" ?>
<protocols>
  <protocol id="0">
    <requests>
      <store command="store: ">^(store:)(\s+)(.+)$</store>
      <remove command="remove: ">^(remove:)(\s+)(\d+)$</remove>
      <search command="search: ">^(search:)(\s+)(\d+)$</search>
    </requests>
    <responses>
      <key command="key: ">^(key:)(\s+)(\d+)$</key>
      <value command="value: ">^(value:)(\s+)(.+)$</value>
      <error command="error: ">^(error:)(\s+)(.+)</error>
    </responses>
  </protocol>
</protocols>